<div class="card-container">
  {{#each items}}
    {{> partialCard}}
  {{/each}}
</div>
<div class="back-links">
  {{#if items.0.followers}}
    <a href="/">Back to Home</a>
  {{/if}}
  {{#unless items.0.followers}}
    <a onclick="window.history.back()">Back to Artist</a>
    <a href="/">Back to Home</a>
  {{/unless}}
</div>
